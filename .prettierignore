# Ignore directories
node_modules
.next
.turbo
out
build
dist

# Ignore lock files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Ignore environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
